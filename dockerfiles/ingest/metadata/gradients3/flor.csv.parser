#!/usr/bin/env python3
import click
import os
import shutil
import sys


@click.command()
@click.argument('infile', type=click.Path(exists=True), nargs=1)
@click.argument('outfile', type=click.Path(), nargs=1)
def main(infile, outfile):
    shutil.copyfile(infile, outfile)    


if __name__ == '__main__':
    main()
